<h1>Account</h1>
<%= form_for Account.new do |f| %>
  <%= f.text_field :name %>
  <%= f.text_field :email %>
  <%= f.text_field :about %>
  <%= f.submit %>
<% end %>
<table>
    <% @accounts.each do |account|%>
      <tr>
        <td><%= link_to account.name, account_path(account) %></td>
        <td><%= account.type.to_s %></td>
      </tr>
    <% end %>
</table>
